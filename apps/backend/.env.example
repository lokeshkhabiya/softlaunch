E2B_API_KEY=your_e2b_api_key_here

# LLM Provider Configuration
# Options: 'openrouter' (default), 'anthropic', 'openai'
LLM_PROVIDER=openrouter

# Model to use (format depends on provider)
# OpenRouter: anthropic/claude-sonnet-4-20250514, openai/gpt-4o, etc.
# Anthropic: claude-sonnet-4-20250514, claude-3-5-haiku-latest, etc.
# OpenAI: gpt-4o, gpt-4-turbo, etc.
LLM_MODEL=anthropic/claude-sonnet-4-20250514

# API Keys (only the one for your chosen provider is required)
OPENROUTER_API_KEY=your_openrouter_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here
OPENAI_API_KEY=your_openai_api_key_here

PORT=

TEMPLATE_ID=

SANDBOX_PORT=

# Cloudflare R2 Configuration
R2_ACCOUNT_ID=your_cloudflare_account_id
R2_ACCESS_KEY_ID=your_r2_access_key_id
R2_SECRET_ACCESS_KEY=your_r2_secret_access_key
R2_BUCKET_NAME=your_bucket_name