E2B_API_KEY=your_e2b_api_key_here

# LLM Provider Configuration
# Options: 'openrouter' (default), 'anthropic', 'openai'
LLM_PROVIDER=openrouter

# OpenRouter Configuration
OPENROUTER_API_KEY=your_openrouter_api_key_here
OPENROUTER_MODEL=anthropic/claude-sonnet-4-20250514

# Anthropic Configuration
ANTHROPIC_API_KEY=your_anthropic_api_key_here
ANTHROPIC_MODEL=claude-sonnet-4-20250514

# OpenAI Configuration
# For Codex models (e.g., gpt-5.1-codex-max), the Responses API is automatically enabled
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-4o

PORT=

TEMPLATE_ID=

SANDBOX_PORT=

# Cloudflare R2 Configuration
R2_ACCOUNT_ID=your_cloudflare_account_id
R2_ACCESS_KEY_ID=your_r2_access_key_id
R2_SECRET_ACCESS_KEY=your_r2_secret_access_key
R2_BUCKET_NAME=your_bucket_name
# Optional: Public URL for R2 bucket (for project thumbnails)
# Can be a R2.dev URL (https://pub-xxx.r2.dev) or custom domain
R2_PUBLIC_URL=

# Langfuse Observability & AI Evals
# Get your credentials at https://cloud.langfuse.com
LANGFUSE_PUBLIC_KEY=pk-lf-...
LANGFUSE_SECRET_KEY=sk-lf-...
LANGFUSE_HOST=https://cloud.langfuse.com